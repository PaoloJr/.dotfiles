# navigation
alias ..='cd ..'
alias ...='cd ../..'
alias ls='ls --color=auto'
alias la='ls -A'
alias ll='ls -laF'
# WSL
alias cdgw='cd /mnt/c/Users/$USER/Documents/GitHub'

# git
alias gs='git status'
alias ga='git add'
alias gc='git commit'

# system
alias update='sudo apt update'
alias upgrade='sudo apt upgrade'

# misc
alias cls='clear'
alias h='history'
alias myipa='curl -s https://ipinfo.io/json | jq -r .'
alias myip='curl -s https://api.ipify.org/; echo'
